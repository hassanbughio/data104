	<package name="default" extends="struts-default">
		<action name="user-list"
			class="com.usermanagement.action.hibernate.UserManagementAction"
			method="getList">
			<result name="list">/user-list.jsp</result>
		</action>
		<action name="adduser">
			<result>/user-form.jsp</result>
		</action>

		<action name="edit"
			class="com.usermanagement.action.hibernate.UserManagementAction"
			method="showEditUser">
			<result name="form">/user-form.jsp</result>
		</action>

		<action name="update"
			class="com.usermanagement.action.hibernate.UserManagementAction"
			method="updateUser">
			<result name="list">/user-list.jsp</result>
		</action>

	<action name="delete"
			class="com.usermanagement.action.hibernate.UserManagementAction"
			method="deleteUser">
			<result name="list">/user-list.jsp</result>
		</action>
		
			<action name="create"
			class="com.usermanagement.action.hibernate.UserManagementAction"
